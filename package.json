{
  "name": "adonis-binding-resolver",
  "version": "2.0.0",
  "description": "Bindings resolver for AdonisJs",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "node --harmony-async-await ./node_modules/.bin/japa",
    "test": "./node_modules/.bin/nyc npm run test:local",
    "test:win": "node --harmony-async-await ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-binding-resolver.git"
  },
  "keywords": [
    "adonisjs",
    "ioc"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-binding-resolver/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-binding-resolver#readme",
  "devDependencies": {
    "adonis-fold": "git+https://github.com/poppinss/adonis-fold.git#dawn",
    "coveralls": "^2.13.0",
    "cz-conventional-changelog": "^2.0.0",
    "japa": "^1.0.3",
    "japa-cli": "^1.0.1",
    "nyc": "^10.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
